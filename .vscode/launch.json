{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run WebServer",
      "program": "${workspaceRoot}/app/index.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Scrapping",
      "program": "${workspaceRoot}/scripts/scrape.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Product Scrapping",
      "program": "${workspaceRoot}/scripts/scrape.js",
      "args": [
        "-p"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Source Scrapping",
      "program": "${workspaceRoot}/scripts/scrape.js",
      "args": [
        "-s"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug GPU Specs Scrapping",
      "program": "${workspaceRoot}/scripts/scrape.js",
      "args": [
        "-g"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug CPU Specs Scrapping",
      "program": "${workspaceRoot}/scripts/scrape.js",
      "args": [
        "-c"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "----"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Matcher",
      "program": "${workspaceRoot}/scripts/match.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "----"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Item Processor",
      "program": "${workspaceRoot}/scripts/itemProcessor.js"
    }
  ]
}
